                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Clip the polygon feature classes to the clipped EnviroAtlas community boundary.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2019-04-09T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Dissolve each piece based on grid_code.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2019-04-09T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
