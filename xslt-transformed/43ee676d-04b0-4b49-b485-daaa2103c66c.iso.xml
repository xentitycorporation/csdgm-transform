                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2019-04-09T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Reclassify the Focal Statistics into 5 classes at 20% intervals.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2019-04-09T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
