         <gmd:lineage>
            <gmd:LI_Lineage>
               <gmd:statement>
                  <gco:CharacterString>This lineage was generated by the FGDC CSDGM to 19139 transformation</gco:CharacterString>
               </gmd:statement>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Acquired NHDPlusV2.1 catseed grid.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2013-06-18T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Acquired 2011 CDL-NLCD raster. The CDL-NLCD, which was developed by the EnviroAtlas team, is a hybrid of the 2011 CDL produced by the National Agricultural Statistics Service (NASS) and the 2011 NLCD produced by the Multi-Resolution Land Characteristics Consortium</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-10-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Reclassify CDL-NLCD 2011 water and wetland classes (11, 90, 95) to a value of 1, and all other classes to 0.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Reclassify NHDPlusV2.1 catseed grid so that all NHD flowline cells have a value of 1 all other values are 0.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Add the NHDPlus catseed grid and the reclassified CDL-NLCD grid using the Plus function in ArcGIS.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>In the new raster output by the Plus function, reclassify values 1 and 2 to 1, all other values to 0.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Reclassify values 1 to 0, and all other values to nodata. The result is the Water Mask, a raster with values of 0 for water and NoData for all other cells.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Calculate the distance to water for each grid pixel by calculating the Euclidean Distance to water using Water Mask raster.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Create integer distance to water layer using Euclidean Distance raster. This step helps to reduce the storage space required by the grid.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Reclassify values 0-108 to value 1 and all other cell values to 0.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
               <gmd:processStep>
                  <gmd:LI_ProcessStep>
                     <gmd:description>
                        <gco:CharacterString>Reclassify values 1 to 1 and all other values to nodata to create the Riparian Zone Mask.</gco:CharacterString>
                     </gmd:description>
                     <gmd:dateTime>
                        <gco:DateTime>2015-11-02T00:00:00</gco:DateTime>
                     </gmd:dateTime>
                  </gmd:LI_ProcessStep>
               </gmd:processStep>
            </gmd:LI_Lineage>
         </gmd:lineage>
